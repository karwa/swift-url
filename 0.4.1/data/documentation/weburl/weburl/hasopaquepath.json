pathComponents-swift.property","type":"reference","isActive":true},{"type":"text","text":" view."}],"type":"paragraph"},{"level":3,"anchor":"Examples","text":"Examples","type":"heading"},{"rows":[[[{"inlineContent":[{"type":"text","text":"URL"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Path","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"Style","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"code":"https:\/\/github.com\/karwa\/swift-url","type":"codeVoice"}],"type":"paragraph"}],[{"inlineContent":[{"code":"\/karwa\/swift-url","type":"codeVoice"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"List"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"codeVoice","code":"file:\/\/\/var\/tmp\/some%20file"}],"type":"paragraph"}],[{"inlineContent":[{"code":"\/var\/tmp\/some%20file","type":"codeVoice"}],"type":"paragraph"}],[{"inlineContent":[{"text":"List","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"codeVoice","code":"my.app:\/profile\/settings"}],"type":"paragraph"}],[{"inlineContent":[{"type":"codeVoice","code":"\/profile\/settings"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"List"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"codeVoice","code":"mailto:bob@example.com"}],"type":"paragraph"}],[{"inlineContent":[{"type":"codeVoice","code":"bob@example.com"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Opaque"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"codeVoice","code":"javascript:alert(\"hello\");"}],"type":"paragraph"}],[{"inlineContent":[{"type":"codeVoice","code":"alert(\"hello\")"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Opaque"}],"type":"paragraph"}]],[[{"inlineContent":[{"code":"data:text\/plain;base64,...","type":"codeVoice"}],"type":"paragraph"}],[{"inlineContent":[{"code":"text\/plain;base64,...","type":"codeVoice"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"Opaque"}],"type":"paragraph
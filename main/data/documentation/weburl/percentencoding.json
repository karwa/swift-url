],"conformance":{"availabilityPrefix":[{"type":"text","text":"Available when"}],"conformancePrefix":[{"type":"text","text":"Conforms when"}],"constraints":[{"code":"Self","type":"codeVoice"},{"text":" is ","type":"text"},{"code":"URLEncodeSet.Component","type":"codeVoice"},{"text":".","type":"text"}]},"abstract":[{"text":"The component percent-encode set. This set includes almost all special characters, making","type":"text"},{"text":" ","type":"text"},{"text":"it suitable for encoding arbitrary strings at runtime for use in any URL component.","type":"text"}],"role":"symbol","url":"\/documentation\/weburl\/percentencodeset\/urlcomponentset","type":"topic","title":"urlComponentSet","identifier":"doc:\/\/com.karwa.WebURL\/documentation\/WebURL\/PercentEncodeSet\/urlComponentSet"},"doc://com.karwa.WebURL/documentation/WebURL/SubstitutionMap":{"abstract":[{"text":"A bidirectional map of ASCII code-points.","type":"text"}],"navigatorTitle":[{"text":"SubstitutionMap","kind":"identifier"}],"identifier":"doc:\/\/com.karwa.WebURL\/documentation\/WebURL\/SubstitutionMap","kind":"symbol","role":"symbol","type":"topic","fragments":[{"text":"protocol","kind":"keyword"},{"text":" ","kind":"text"},{"text":"SubstitutionMap","kind":"identifier"}],"title":"SubstitutionMap","url":"\/documentation\/weburl\/substitutionmap"},"https://url.spec.whatwg.org/#application/x-www-form-urlencoded":{"titleInlineContent":[{"text":"application\/x-www-form-urlencoded","type":"text"}],"identifier":"https:\/\/url.spec.whatwg.org\/#application\/x-www-form-urlencoded","title":"application\/x-www-form-urlencoded","type":"link","url":"https:\/\/url.spec.whatwg.org\/#application\/x-www-form-urlencoded"}}}
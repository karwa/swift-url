dentifier":"s:6WebURL12URLEncodeSetO12FormEncodingV","text":"FormEncoding","kind":"typeIdentifier"}],"kind":"symbol","url":"\/documentation\/weburl\/percentencodeset\/formencoding","abstract":[{"text":"Form-encoding. This encode-set is used to encode ","type":"text"},{"identifier":"https:\/\/url.spec.whatwg.org\/#application\/x-www-form-urlencoded","type":"reference","isActive":true},{"type":"text","text":" "},{"type":"text","text":"content. It is unique in that it performs substitutions as well as percent-encoding."}],"conformance":{"availabilityPrefix":[{"type":"text","text":"Available when"}],"constraints":[{"code":"Self","type":"codeVoice"},{"type":"text","text":" is "},{"code":"URLEncodeSet.FormEncoding","type":"codeVoice"},{"type":"text","text":"."}],"conformancePrefix":[{"type":"text","text":"Conforms when"}]},"role":"symbol","title":"formEncoding"},"doc://com.karwa.WebURL/documentation/WebURL/LazilyPercentEncoded":{"abstract":[{"text":"A ","type":"text"},{"type":"codeVoice","code":"Collection"},{"text":" which percent-encodes elements from its ","type":"text"},{"type":"codeVoice","code":"Source"},{"text":" on-demand using a given ","type":"text"},{"type":"codeVoice","code":"EncodeSet"},{"text":".","type":"text"}],"navigatorTitle":[{"text":"LazilyPercentEncoded","kind":"identifier"}],"identifier":"doc:\/\/com.karwa.WebURL\/documentation\/WebURL\/LazilyPercentEncoded","kind":"symbol","role":"symbol","type":"topic","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"LazilyPercentEncoded","kind":"identifier"}],"title":"LazilyPercentEncoded","url":"\/documentation\/weburl\/lazilypercentencoded"},"doc://com.karwa.WebURL/documentation/WebURL/WebURL/pathComponents-swift.property":{"role":"symbol","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"A read-write view of the URL’s path components."}],"identifier":"doc:\/\/com.karwa.WebURL\/documentation\/WebURL\/WebURL\/pathComponents-swift.property","fragments":[{"kind":"k